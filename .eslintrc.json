{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["bundle.js"],
    "rules": {
        "no-alert": "off",
        "no-restricted-globals": "off",
        "import/no-mutable-exports": "off",
        "consistent-return": "off",
        "import/no-cycle": "off",
        "no-import-assign": "off"
    }
}
